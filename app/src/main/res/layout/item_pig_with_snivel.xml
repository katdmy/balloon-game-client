<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <com.sdsmdg.harjot.vectormaster.VectorMasterView
        android:id="@+id/vPeppe"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:vector_src="@drawable/ic_peppe" />

    <com.katdmy.android.balloon_game_client.utils.CustomSnivel
        android:id="@+id/vSnivel"
        android:layout_width="0dp"
        android:layout_height="95dp"
        android:paddingEnd="16dp"
        app:layout_constraintBottom_toBottomOf="@id/vPeppe"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@id/vPeppe"
        app:layout_constraintTop_toTopOf="@id/vPeppe"
        app:layout_constraintVertical_bias="0.0"
        app:snivelX="100dp"
        tools:ignore="RtlSymmetry" />

</androidx.constraintlayout.widget.ConstraintLayout>